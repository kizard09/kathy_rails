<!DOCTYPE html>
<html>
<head>
      <title><%= @title %></title>
      <% meta_tag :description, @description %>
      <% meta_tag :keywords, @keywords %>
        <meta name='description'
      content='<%= yield_meta_tag(:description, 'Default description') %>'>
        <meta name='keywords'
      content='<%= yield_meta_tag(:keywords, 'Default description') %>'>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
         <%= favicon_link_tag 'favicon.ico' %>
   <link href="https://use.fontawesome.com/releases/v5.0.3/css/all.css" rel="stylesheet">

         <link type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.1.6/assets/owl.carousel.min.css" rel="stylesheet" >
         <link type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.1.6/assets/owl.theme.default.min.css" rel="stylesheet" >
          <link type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css" rel="stylesheet" >
          <link type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" rel="stylesheet">
        <script rel="stylesheet" src="https://use.typekit.net/zid6riz.js"></script>
        <script>try{Typekit.load({ async: true });}catch(e){}</script>
      
        <%= stylesheet_link_tag   'application', media: 'all', 'data-turbolinks-track' => true %>     

   <%= csrf_meta_tags %>
   <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-86356556-1', 'auto');
  ga('send', 'pageview');

</script>
</head>
<body>

<div class="r">
    <nav>
      <div class="mobile">
        <a href="/" class="home"><%= image_tag 'Logo_Mark2_x1.png' %></a>
        <a href="/" class="mobile name">Kathy Izard</a>
        <div id="menu">
          <span></span>
          <span></span>
          <span></span>
          <span></span>
        </div>
      </div>
      <div class="nav">
        <div class="secondary">
          <div class="wrap cf">
            <div class="left ">
            <div class="social">
              <a href="https://facebook.com/kathyizardclt" class="fab fa-facebook" ></a>
              <a class="fab fa-twitter" href="https://twitter.com/kathyizardclt" target="_blank"></a>
              <a class="fab fa-instagram" href="https://www.instagram.com/kathyizardclt/" target="_blank"></a>
                <%= link_to 'connect', site_contact_path, class: 'button' %>
              </div>
            </div>

            <a href="/" class="home tablet"><%= image_tag 'Logo_4.png' %></a>
            <div class="right">
              
              <%= link_to 'Contact', site_contact_path %>
              
            </div>
          </div>
        </div>
        <div class="primary">
          <div class="wrap">
            <%= link_to 'About Me', site_aboutme_path %>
            <%= link_to 'Book', site_book_path %>
            <%= link_to 'Moore Place', site_mooreplace_path %>
            <%= link_to 'About You', site_aboutyou_path %>
            <%= link_to 'Stories', site_readersstories_path %>
            <%= link_to 'Speaking', site_speaking_path %>
            <%= link_to 'Media', site_media_path %>
          </div>
        </div>
      </div>
    </nav>
  </div>
  
  <%= yield %>

<footer>
      <div class="wrap cf">
        <a href="/" class='home'><%= image_tag 'Logo_Mark2_x1.png' %></a>
       
        <div class="primary desktop">
          <div class="wrap">
            <%= link_to 'About Me', site_aboutme_path %>
            <%= link_to 'Book', site_book_path %>
            <%= link_to 'Moore Place', site_mooreplace_path %>
            <%= link_to 'About You', site_aboutyou_path %>
            <%= link_to 'Reader Stories', site_readersstories_path %>
            <%= link_to 'Speaking', site_speaking_path %>
          </div>
        </div>
        <div class="social">
          <a href="https://facebook.com/kathyizardclt" class="fab fa-facebook" ></a>
              <a class="fab fa-twitter" href="https://twitter.com/kathyizardclt" target="_blank"></a>
              <a class="fab fa-instagram" href="https://www.instagram.com/kathyizardclt/" target="_blank"></a>
        </div>
      </div>
    </footer>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
  <%= javascript_include_tag Ckeditor.cdn_url %>
   <%= javascript_include_tag 'jquery.fancybox.pack.js', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'scripts', 'data-turbolinks-track' => true %>

  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.1.6/owl.carousel.min.js"></script>

</body>
</html>
